set t_Co=256
set background=dark
set listchars=tab:\|\ ,eol:¬,extends:❯,precedes:❮,trail:⌴
set showbreak=↪\ 
set colorcolumn=+1
set cursorline

" Block cursor on normal mode, underline on insert mode
let &t_SI = "\e[3 q"
let &t_EI = "\e[2 q"

let g:airline_theme='monochrome'

silent! colorscheme PaperColor

" vim: ft=vim
